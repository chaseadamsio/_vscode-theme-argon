{
    "name": "{{theme}}",
    "type": "{{type}}",
    "colors": {
        "background": "{{background}}",
        "foreground": "{{foreground}}",
        "textLink.foreground": "{{blue}}",
        "textLink.activeForeground": "{{blue}}33",
        "welcomePage.buttonHoverBackground": "{{gray00}}",
        "welcomePage.buttonBackground": "{{black}}",
        "editor.background": "{{background}}",
        "editor.foreground": "{{foreground}}",
        "editor.findMatchBackground": "{{blue}}33",
        "editor.findMatchHighlightBackground": "{{blue}}55",
        "editor.findRangeHighlightBackground": "{{cyan}}33",
        "editor.hoverHighlightBackground": "{{gray00}}",
        "editor.lineHighlightBackground": "{{gray01}}",
        "editorBracketMatch.border": "{{foreground-muted}}",
        "editorCodeLens.foreground": "{{gray04}}",
        "editorCursor.foreground": "{{blue}}",
        "editorError.foreground": "{{red}}",
        "editorGutter.background": "{{background}}",
        "editorGutter.addedBackground": "{{green}}",
        "editorGutter.modifiedBackground": "{{blue}}",
        "editorGutter.deletedBackground": "{{red}}",
        "editorLineNumber.foreground": "{{gray04}}",
        "editorLineNumber.activeForeground": "{{foreground-muted}}",
        "editorLink.activeForeground": "{{cyan}}",
        "editorGroupHeader.tabsBackground": "{{gray00}}",
        "editorHoverWidget.background": "{{gray00}}",
        "editorHoverWidget.border": "{{gray03}}",
        "editorSuggestWidget.background": "{{background}}",
        "editorSuggestWidget.border": "{{gray03}}",
        "editorSuggestWidget.foreground": "{{foreground}}",
        "editorSuggestWidget.highlightForeground": "{{blue}}",
        "editorSuggestWidget.selectedBackground": "{{gray00}}",
        "peekView.order": "{{gray00}}",
        "peekViewEditor.background": "{{gray00}}",
        "peekViewEditor.matchHighlightBackground": "{{magenta}}33",
        "peekViewResult.background":"{{gray00}}",
        "peekViewResult.fileForeground": "{{foreground-muted}}",
        "peekViewResult.selectionBackground": "{{background}}",
        "peekViewResult.selectionForeground": "{{foreground}}",
        "peekViewResult.lineForeground": "{{foreground-muted}}",
        "peekViewResult.matchHighlightBackground": "{{magenta}}33",
        "peekViewTitle.background": "{{gray00}}",
        "peekViewTitleDescription.foreground": "{{foreground}}",
        "peekViewTitleLabel.foreground": "{{foreground}}",
        "scrollbar.shadow": "{{background}}",
        "selection.background": "{{blue}}88"
    },
    "tokenColors": [

        {
            "name": "Comment plus start of comment",
            "scope": "comment, punctuation.definition.comment",
            "settings": {
                "foreground": "{{gray04}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Text",
            "scope": "support.function",
            "settings": {
                "foreground": "{{blue}}"
            }
        },
        {
            "name": "",
            "scope": "storage.type",
            "settings": {
                "foreground": "{{magenta}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "",
            "scope": "support.class",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "",
            "scope": "punctuation.definition.template-expression",
            "settings": {
                "foreground": "{{green}}",
                "fontStyle": "bold"
            }
        },
        {
            "name": "",
            "scope": "meta.brace.round",
            "settings": {
                "foreground": "{{foreground}}"
            }
        },
        {
            "name": "Boolean",
            "scope": "constant.language.boolean",
            "settings": {
                "fontStyle": "bold",
                "foreground": "{{foreground}}"
            }
        },
        {
            "name": "",
            "scope": ["storage.type.string.go"],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "",
            "scope": "variable.other.object",
            "settings": {
                "foreground": "{{cyan}}",
                "fontStyle": "bold"
            }
        },
        {
            "name": "",
            "scope": "variable.object.property",
            "settings": {
                "foreground": "{{foreground}}",
                "fontStyle": "bold"
            }
        },
        {
            "name": "",
            "scope": "meta.import variable.other",
            "settings": {
                "foreground": "{{cyanAlt}}",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
              "foreground": "{{magenta}}"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "{{green}}"
            }
        },
        {
            "name": "Function",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "{{cyanAlt}}"
            }
        },
        {
            "name": "attribute name",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "{{cyanAlt}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "tag",
            "scope": "entity.name.tag, support.type.property-name",
            "settings": {
                "foreground": "{{magenta}}"
            }
        },
        {
            "name": "tag",
            "scope": "source.css keyword.other.unit",
            "settings": {
                "foreground": "{{foreground}}"
            }
        },
        {
            "name": "tag",
            "scope": "support.constant.property-value",
            "settings": {
                "foreground": "{{cyanAlt}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "tag",
            "scope": "meta.object-literal.key",
            "settings": {
                "foreground": "{{magenta}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Doctype",
            "scope": "meta.tag.sgml.doctype",
            "settings": {
                "foreground": "{{cyan}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "[XML] Doctype Tag",
            "scope": "meta.tag.sgml.doctype punctuation.definition.tag",
            "settings": {
                "foreground": "{{foreground}}"
            }
        },
        {
            "name": "[XML] Document Root Variable",
            "scope": "meta.tag.sgml.doctype variable.language.documentroot",
            "settings": {
                "foreground": "{{yellow}}"
            }
        },
        {
            "name": "css tag",
            "scope": "entity.name.tag.css",
            "settings": {
                "foreground": "{{yellow}}"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "{{magenta}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup Heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "{{cyan}}",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup Italic",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup Bold",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup Separator",
            "scope": "meta.separator",
            "settings": {
                "foreground": "{{cyan}}"
            }
        },
        {
            "name": "Markup Link ",
            "scope": "meta.link.reference.def",
            "settings": {
                "foreground": "{{magenta}}"
            }
        },
        {
            "name": "Markup List",
            "scope": "beginning.punctuation.definition.list",
            "settings": {
                "foreground": "{{green}}"
            }
        },
        {
            "name": "Markup Link Title",
            "scope": "string.other.link.title",
            "settings": {
                "foreground": "{{green}}"
            }
        },
        {
            "name": "Markup Link URL",
            "scope": "markup.underline.link",
            "settings": {
                "foreground": "{{cyanAlt}}"
            }
        },
        {
            "name": "Markup Code",
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "{{yellow}}"
            }
        },
        {
            "name": "Org Heading",
            "scope": "entity.name.type.org",
            "settings": {
                "foreground": "{{cyan}}",
                "fontStyle": "bold"
            }
        },
        {
            "name": "[Org] blocks",
            "scope": "punctuation.definition.tag.org",
            "settings": {
                "foreground": "{{gray04}}"
            }
        },
        {
            "name": "[Org] block language",
            "scope": "block.language.org",
            "settings": {
                "foreground": "{{green}}99",
                "fontStyle": "italic"
            }
        },
        {
            "name": "[Org] Keyword Key",
            "scope": "support.type.property-name.org",
            "settings": {
                "foreground": "{{cyan}}"
            }
        },
        {
            "name": "[Org] Keyword Value",
            "scope": "meta.structure.dictionary.value.org",
            "settings": {
                "foreground": "{{green}}"
            }
        },
        {
            "name": "YAML List Punctuation",
            "scope": "punctuation.definition.block.sequence.item",
            "settings": {
                "foreground": "{{green}}"
            }
        },
        {
            "name": "[Go] function definition",
            "scope": "entity.name.function.go",
            "settings": {
                "foreground": "{{cyan}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "[Go] function definition",
            "scope": "support.function.go",
            "settings": {
                "foreground": "{{cyanAlt}}"
            }
        },
        {
            "name": "YAML string",
            "scope": "string.quoted.double.yaml, string.quoted.single.yaml",
            "settings": {
                "foreground": "{{green}}"
            }
        },
        {
            "name": "YAML unquoted",
            "scope": "string.unquoted.plain.out.yaml",
            "settings": {
                "foreground": "{{white}}"
            }
        },
        {
            "name": "INI group title",
            "scope": "entity.name.section.group-title.ini, punctuation.definition.entity.ini",
            "settings": {
                "foreground": "{{cyanAlt}}"
            }
        },
        {
            "name": "[VSCODE-CUSTOM] JSON Property Name",
            "scope": "support.type.property-name.json",
            "settings": {
                "foreground": "{{magenta}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "[VSCODE-CUSTOM] JSON constants",
            "scope": "constant.language.json",
            "settings": {
                "foreground": "{{cyanAlt}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "[VSCODE-CUSTOM] JSON Property Value",
            "scope": "string.quoted.double.json",
            "settings": {
                "foreground": "{{green}}"
            }
        },
        {
            "name": "[Typescript] Object",
            "scope": "meta.function-call entity.name.function",
            "settings": {
                "foreground": "{{foreground}}",
                "fontStyle": "bold"
            }
        },

        {
            "name": "[Typescript] Object",
            "scope": "variable.other.object.ts",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "[Typescript] Object",
            "scope": "variable.other.object.property.ts",
            "settings": {
                "foreground": "{{cyan}}"
            }
        },
        {
            "name": "[Typescript] Object",
            "scope": "meta.parameters.ts",
            "settings": {
                "foreground": "{{foreground}}",
                "fontStyle": "bold"
            }
        },
        {
            "name": "[Typescript] Object",
            "scope": "meta.type.annotation.ts",
            "settings": {
                "foreground": "{{cyan}}",
                "fontStyle": "italic"
            }
        }
    ]
}
