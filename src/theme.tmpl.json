    "tokenColors": [
        
        {
            "name": "Comment",
            "scope": "comment,punctuation.definition.comment",
            "settings": {
                "foreground": "{{gray04}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Text",
            "scope": "support.function",
            "settings": {
                "foreground": "{{blue}}"
            }
        },
        {
            "name": "",
            "scope": "storage.type",
            "settings": {
                "foreground": "{{magenta}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "",
            "scope": "support.class",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Boolean",
            "scope": "constant.language.boolean",
            "settings": {
                "foreground": "{{yellow}}"
            }
        },
        {
            "name": "",
            "scope": ["storage.type.string.go"],
            "settings": {
                "foreground": "{{yellow}}"
            }
        },
        {
            "name": "",
            "scope": "variable.other.object.js",
            "settings": {
                "foreground": "{{foreground}}",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Boolean",
            "scope": "none",
            "settings": {
              "foreground": "{{red}}"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
              "foreground": "{{magenta}}"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "{{green}}"
            }
        },
        {
            "name": "Function",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "{{blue}}"
            }
        },
        {
            "name": "attribute name",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "{{blue}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "tag",
            "scope": "entity.name.tag, support.type.property-name",
            "settings": {
                "foreground": "{{magenta}}"
            }
        },
        {
            "name": "tag",
            "scope": "source.css keyword.other.unit",
            "settings": {
                "foreground": "{{foreground}}"
            }
        },
        {
            "name": "tag",
            "scope": "support.constant.property-value",
            "settings": {
                "foreground": "{{blue}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "tag",
            "scope": "meta.object-literal.key",
            "settings": {
                "foreground": "{{magenta}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "css tag",
            "scope": "entity.name.tag.css",
            "settings": {
                "foreground": "{{yellow}}"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "{{magenta}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup Heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "{{cyan}}",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup Italic",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup Bold",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup Separator",
            "scope": "meta.separator",
            "settings": {
                "foreground": "{{cyan}}"
            }
        },
        {
            "name": "Markup Link ",
            "scope": "meta.link.reference.def",
            "settings": {
                "foreground": "{{magenta}}"
            }
        },
        {
            "name": "Markup List",
            "scope": "beginning.punctuation.definition.list",
            "settings": {
                "foreground": "{{green}}"
            }
        },
        {
            "name": "Markup Link Title",
            "scope": "string.other.link.title",
            "settings": {
                "foreground": "{{green}}"
            }
        },
        {
            "name": "Markup Link URL",
            "scope": "markup.underline.link",
            "settings": {
                "foreground": "{{blue}}"
            }
        },
        {
            "name": "Markup Code",
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "{{yellow}}"
            }
        },
        {
            "name": "[Org] Bold",
            "scope": "markup.bold.org",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "[Org] Variable (links)",
            "scope": "variable.org",
            "settings": {
                "foreground": "{{red}}"
            }
        },
        {
            "name": "[Org] blocks",
            "scope": "punctuation.definition.tag.org",
            "settings": {
                "foreground": "{{gray04}}"
            }
        },
        {
            "name": "[Org] block language",
            "scope": "block.language.org",
            "settings": {
                "foreground": "{{green}}99",
                "fontStyle": "italic"
            }
        },
        {
            "name": "YAML List Punctuation",
            "scope": "punctuation.definition.block.sequence.item",
            "settings": {
                "foreground": "{{green}}"
            }
        },
        {
            "name": "[Go] function definition",
            "scope": "entity.name.function.go",
            "settings": {
                "foreground": "{{cyan}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "[Go] function definition",
            "scope": "support.function.go",
            "settings": {
                "foreground": "{{blue}}"
            }
        },
        {
            "name": "[Go] variable assignment",
            "scope": "variable.other.assignment.go",
            "settings": {
                "foreground": "{{red}}"
            }
        },
        {
            "name": "YAML string",
            "scope": "string.quoted.double.yaml, string.quoted.single.yaml",
            "settings": {
                "foreground": "{{green}}"
            }
        },
        {
            "name": "YAML unquoted",
            "scope": "string.unquoted.plain.out.yaml",
            "settings": {
                "foreground": "{{white}}"
            }
        },
        {
            "name": "INI group title",
            "scope": "entity.name.section.group-title.ini, punctuation.definition.entity.ini",
            "settings": {
                "foreground": "{{blue}}"
            }
        },
        {
            "name": "[VSCODE-CUSTOM] JSON Property Name",
            "scope": "support.type.property-name.json",
            "settings": {
                "foreground": "{{magenta}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "[VSCODE-CUSTOM] JSON constants",
            "scope": "constant.language.json",
            "settings": {
                "foreground": "{{blue}}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "[VSCODE-CUSTOM] JSON Property Value",
            "scope": "string.quoted.double.json",
            "settings": {
                "foreground": "{{green}}"
            }
        },
        {
            "name": "[Typescript] Object",
            "scope": "meta.function-call entity.name.function",
            "settings": {
                "foreground": "{{foreground}}",
                "fontStyle": "bold"
            }
        },

        {
            "name": "[Typescript] Object",
            "scope": "variable.other.object.ts",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "[Typescript] Object",
            "scope": "variable.other.object.property.ts",
            "settings": {
                "foreground": "{{cyan}}"
            }
        },
        {
            "name": "[Typescript] Object",
            "scope": "meta.parameters.ts",
            "settings": {
                "foreground": "{{foreground}}",
                "fontStyle": "bold"
            }
        },
        {
            "name": "[Typescript] Object",
            "scope": "meta.type.annotation.ts",
            "settings": {
                "foreground": "{{cyan}}",
                "fontStyle": "italic"
            }
        }
    ]
}
